<h1>Sign up</h1>
<%= form_with(model: @user, local: true) do |f| %>
<div class="form-group">
  <%= f.label :username %>
  <%= f.text_field :username %>
</div>
<div class="form-group">
  <%= f.label :password %>
  <%= f.password_field :password %>
</div>

<div class="form-group">
<%= f.submit "Sign up" %>
</div>
<% end %>
